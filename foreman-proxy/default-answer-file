# hostname of this server, default is value returned by hostname
servername =

# port number
port = 9090

# puppetca (true or false)
puppetca = false

# manage DHCP (default: true)
dhcp = false

# List of interfaces for which DHCP will be set up (default "eth1")
dhcp_interface = eth1,

# DHCP gateway
dhcp_gateway = '192.168.100.1'

# DHCP range
dhcp_range = '192.168.100.50 192.168.100.200'

# Enable DNS (default: true)
dns = true

# List of interfaces for which DNS will be set up (default "eth1")
dns_interface = eth1

# Enable TFTP (default: true)
tftp = true

